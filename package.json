{
  "name": "astraicons",
  "version": "1.6.7",
  "type": "module",
  "license": "MIT",
  "description": "A comprehensive icon set designed for websites, applications, social networks, and print media, tailored specifically for UI Astra.",
  "repository": "https://github.com/uiastra/astraicons.git",
  "keywords": [
    "uiastra",
    "ui-icons",
    "astraicons"
  ],
  "files": [
    "linear/",
    "bold/",
    "brand/",
    "duotone/",
    "3d/",
    "illustrations/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "cleanup": "rimraf ./linear ./vue/linear ./react/linear ./bold ./vue/bold ./react/bold ./brand ./vue/brand ./react/brand ./duotone ./vue/duotone ./react/duotone ./3d ./vue/3d ./react/3d ./illustrations ./vue/illustrations ./react/illustrations ./optimized",
    "cleanup-optimized": "rimraf ./optimized",
    "build": "npm run build-bold && npm run build-linear && npm run build-brand && npm run build-duotone && npm run build-3d && npm run build-illustrations && npm run build-react && npm run build-vue",
    "build-react": "node ./scripts/build.js react",
    "build-vue": "node ./scripts/build.js vue",
    "build-linear": "rimraf ./linear ./optimized/linear && svgo --config=svgo.linear.mjs -f ./icons/linear -o ./optimized/linear --pretty --indent=2 && mkdir -p ./linear && cp -R ./optimized/linear/* ./linear",
    "build-bold": "rimraf ./bold ./optimized/bold && svgo --config=svgo.bold.mjs -f ./icons/bold -o ./optimized/bold --pretty --indent=2 && mkdir -p ./bold && cp -R ./optimized/bold/* ./bold",
    "build-brand": "rimraf ./brand ./optimized/brand && svgo --config=svgo.brand.mjs -f ./icons/brand -o ./optimized/brand --pretty --indent=2 && mkdir -p ./brand && cp -R ./optimized/brand/* ./brand",
    "build-duotone": "rimraf ./duotone ./optimized/duotone && svgo --config=svgo.duotone.mjs -f ./icons/duotone -o ./optimized/duotone --pretty --indent=2 && mkdir -p ./duotone && cp -R ./optimized/duotone/* ./duotone",
    "build-3d": "rimraf ./3d ./optimized/3d && svgo --config=svgo.3d.mjs -f ./icons/3d -o ./optimized/3d --pretty --indent=2 && mkdir -p ./3d && cp -R ./optimized/3d/* ./3d",
    "build-illustrations": "rimraf ./illustrations ./optimized/illustrations && svgo --config=svgo.illustrations.mjs -f ./icons/illustrations -o ./optimized/illustrations --pretty --indent=2 && mkdir -p ./illustrations && cp -R ./optimized/illustrations/* ./illustrations"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@vue/compiler-dom": "^3.5.20",
    "camelcase": "^8.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "svgo": "^4.0.0"
  }
}
